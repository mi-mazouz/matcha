    "react-loadable": "^5.5.0",
    "react-notification-system": "^0.2.17",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "4.0.0",
    "redux-form": "^7.4.2",
    "redux-saga": "^0.16.0",
